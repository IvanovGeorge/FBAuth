# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'FBAuth' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FBAuth
	pod 'FacebookCore'
	pod 'FacebookLogin'
	pod 'FacebookShare'
	pod "Cuckoo"


  target 'FBAuthTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'FacebookCore'
	pod 'FacebookLogin'
	pod 'FacebookShare'
  end

end
